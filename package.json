    "version": "0.0.1",
    "scripts": {
        "lint": "tslint --project tslint.json",
        "start": "webpack-dev-server --port 8080",
        "predeploy": "webpack",
        "deploy:files": "cd ./dist && aws s3 sync --exclude \"index.html\" --cache-control \"max-age=31536000\" . s3://dorota-koszacka-www",
        "deploy:html": "aws s3 cp --cache-control \"max-age=60\" ./dist/index.html s3://dorota-koszacka-www",
        "deploy": "run-p deploy:files deploy:html"
    },
    "devDependencies": {
        "@types/classnames": "^2.2.9",
        "@types/node": "^12.7.1",
        "@types/react": "^16.9.1",
        "@types/react-dom": "^16.8.5",
        "@types/react-image-gallery": "^0.8.4",
        "@types/react-router": "^5.0.3",
        "@types/react-router-dom": "^4.3.4",
        "awesome-typescript-loader": "^5.2.1",
        "classnames": "^2.2.6",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^3.2.0",
        "file-loader": "^4.2.0",
        "html-webpack-plugin": "^3.2.0",
        "node-sass": "^4.12.0",
        "npm-run-all": "^4.1.5",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-image-gallery": "^0.9.0",
        "react-router": "^5.0.1",
        "react-router-dom": "^5.0.1",
        "s3-deploy": "^1.3.0",
        "sass-loader": "^7.2.0",
        "source-map-loader": "^0.2.4",
        "style-loader": "^1.0.0",
        "svg-inline-loader": "^0.8.0",
        "typescript": "^3.5.3",
        "webpack": "^4.39.1",
        "webpack-cli": "^3.3.6",
        "webpack-dev-server": "^3.8.0"
    }
}
