{
    "extends": "tslint:recommended",
    "linterOptions": {
        "include": [
            ".src/**/*.tsx"
        ]
    },
    "rules": {
        "max-line-length": {
            "options": [120]
        },
        "no-console": {
            "severity": "warning",
            "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
        }
    }
}
